{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell Interactive Session",
            "type": "PowerShell",
            "request": "launch",
            "cwd": "${cwd}"
        },
        {
            "name": "Debug Autonomous Optimizer",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/autonomous_optimizer.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": [],
            "justMyCode": false,
            "stopOnEntry": false,
            "showReturnValue": true,
            "subProcess": true
        },
        {
            "name": "Debug Model Optimizer",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/model_optimizer.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": [],
            "justMyCode": false,
            "stopOnEntry": false,
            "showReturnValue": true
        },
        {
            "name": "Debug SOTA Benchmarks",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/sota_benchmark.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": [],
            "justMyCode": false,
            "stopOnEntry": false,
            "showReturnValue": true
        },
        {
            "name": "Debug Test Suite",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test_autonomous_optimizer.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "PYTEST_CURRENT_TEST": "autonomous_optimizer"
            },
            "args": [],
            "justMyCode": false,
            "stopOnEntry": false,
            "showReturnValue": true
        },
        {
            "name": "Debug Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": [],
            "justMyCode": true,
            "stopOnEntry": false,
            "showReturnValue": true
        },
        {
            "name": "Debug Azure Integration",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/azure_config.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "AZURE_CLIENT_ID": "${env:AZURE_CLIENT_ID}",
                "AZURE_CLIENT_SECRET": "${env:AZURE_CLIENT_SECRET}",
                "AZURE_TENANT_ID": "${env:AZURE_TENANT_ID}"
            },
            "args": [],
            "justMyCode": false,
            "stopOnEntry": false,
            "showReturnValue": true
        },
        {
            "name": "Debug Performance Monitor",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/autonomous_optimizer.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "DEBUG_PERFORMANCE": "true",
                "LOG_LEVEL": "DEBUG"
            },
            "args": ["--performance-mode"],
            "justMyCode": false,
            "stopOnEntry": false,
            "showReturnValue": true,
            "subProcess": true
        }
    ]
}
