{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "type": "timerTrigger",
      "name": "timer",
      "direction": "in",
      "schedule": "0 */15 * * * *"
    },
    {
      "type": "cosmosDB",
      "name": "quantumResults",
      "direction": "in",
      "connectionStringSetting": "CosmosDBConnectionString",
      "databaseName": "life-platform-db",
      "collectionName": "quantum-results",
      "sqlQuery": "SELECT * FROM c WHERE c.timestamp > @startTime"
    },
    {
      "type": "blob",
      "name": "modelOutput",
      "direction": "out",
      "path": "models/{datetime}/life-model.pkl",
      "connection": "AzureWebJobsStorage"
    }
  ]
}