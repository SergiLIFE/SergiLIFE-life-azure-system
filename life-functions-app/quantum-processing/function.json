{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "type": "eventHubTrigger",
      "name": "events",
      "direction": "in",
      "eventHubName": "processed-eeg",
      "connection": "EventHubConnectionString",
      "cardinality": "many",
      "consumerGroup": "$Default"
    },
    {
      "type": "cosmosDB",
      "name": "outputDocument",
      "direction": "out",
      "connectionStringSetting": "CosmosDBConnectionString",
      "databaseName": "life-platform-db",
      "collectionName": "quantum-results",
      "createIfNotExists": true
    }
  ]
}